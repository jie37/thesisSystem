<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thesis.server.mapper.SubjectMapper">


    <resultMap id="BaseResultMap" type="com.thesis.server.pojo.Subject">
        <id column="Sub_id" property="subId" />
        <result column="Sub_name" property="subName" />
        <result column="Introduce" property="Introduce" />
        <result column="Part_num" property="partNum" />
        <result column="Postil_flag" property="postilFlag" />
    </resultMap>


    <sql id="Base_Column_List">
        Sub_id, Sub_name, Introduce, Part_num, Postil_flag
    </sql>
    <select id="getThesisByPage" resultMap="BaseResultMap">
        select *
        FROM `subject`
    </select>

</mapper>
